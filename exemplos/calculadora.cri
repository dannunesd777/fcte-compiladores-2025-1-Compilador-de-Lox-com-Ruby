# Calculadora de expressões aritméticas básicas
# Demonstra operações matemáticas fundamentais

# Operações básicas
print 2 + 3
print 10 - 4
print 5 * 6
print 15 / 3

# Operações com precedência
print 2 + 3 * 4
print (2 + 3) * 4
print 2 * 3 + 4 * 5

# Potenciação
print 2 ^ 3
print 3 ^ 2 ^ 2

# Operações com números decimais
print 3.14 * 2
print 10.5 / 2.5
print 1.5 + 2.7

# Expressões complexas
print ((2 + 3) * 4 - 1) / 3
print 2 ^ (3 + 1) - 5 * 2